{
  "name": "ecommerce-pro-backend",
  "version": "1.0.0",
  "description": "Backend API for E-Commerce Pro Platform with super admin features",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/**/*.js",
    "seed:superadmin": "node src/seeds/superAdminSeed.js",
    "seed:categories": "node src/seeds/categoriesSeed.js",
    "seed:products": "node src/seeds/productsSeed.js",
    "seed:all": "npm run seed:superadmin && npm run seed:categories && npm run seed:products",
    "db:migrate": "node src/database/migrate.js",
    "db:reset": "node src/database/reset.js",
    "build": "npm run lint && npm run test",
    "prestart": "npm run build"
  },
  "keywords": [
    "ecommerce",
    "marketplace",
    "super-admin",
    "affiliate",
    "revenue-tracking"
  ],
  "author": "E-Commerce Pro Platform",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "cloudinary": "^1.40.0",
    "stripe": "^13.5.0",
    "nodemailer": "^6.9.6",
    "socket.io": "^4.7.2",
    "moment": "^2.29.4",
    "winston": "^3.10.0",
    "compression": "^1.7.4",
    "express-mongo-sanitize": "^2.2.0",
    "hpp": "^0.2.3",
    "express-useragent": "^1.0.15",
    "geoip-lite": "^1.4.9",
    "ua-parser-js": "^1.0.35",
    "redis": "^4.6.8",
    "axios": "^1.5.0",
    "cron": "^3.0.3",
    "uuid": "^9.0.0",
    "morgan": "^1.10.0",
    "express-session": "^1.17.3",
    "cookie-parser": "^1.4.6",
    "validator": "^13.11.0",
    "sharp": "^0.33.1",
    "fs-extra": "^11.2.0",
    "express-jwt": "^8.4.1",
    "connect-redis": "^7.1.0",
    "connect-mongo": "^5.0.0",
    "response-time": "^2.3.2",
    "swagger-ui-express": "^5.0.0",
    "swagger-jsdoc": "^6.2.8",
    "lodash": "^4.17.21",
    "async": "^3.2.5",
    "csv-parse": "^5.5.0",
    "exceljs": "^4.4.0",
    "yamljs": "^0.3.0",
    "express-status-monitor": "^1.3.4",
    "cls-rtracer": "^2.9.0",
    "currency.js": "^2.0.4",
    "mongoose-paginate-v2": "^1.7.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.4",
    "supertest": "^6.3.3",
    "eslint": "^8.48.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.28.1",
    "prettier": "^3.0.3",
    "cross-env": "^7.0.3",
    "faker": "^5.5.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/ecommerce-pro-platform.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/ecommerce-pro-platform/issues"
  },
  "homepage": "https://github.com/yourusername/ecommerce-pro-platform#readme"
}
